namespace apache {
    class output {
        var func;
        var request;
        var buffer;

        function output(var request) {
            self.request = request;
            self.func = dlfunc(null, "ap_rwrite", 'ipip');
            self.buffer = new 'ByteBuffer';
        }

        function puts(string msg) {
            var write = self.func;
            self.buffer =: msg;
            return write(self.buffer, elements(self.buffer), self.request);
        }
    }   
}